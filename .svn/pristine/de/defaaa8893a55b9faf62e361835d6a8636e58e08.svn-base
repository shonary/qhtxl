.signup-box .signup-content .ct form .field label {
    color: #53575B;
    cursor: text;
    font-size: 14px;
    font-weight: bold;
    height: 32px;
    left: 7px;
    line-height: 32px;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 0;
}

input.text {
    height: 20px;
    line-height: 20px;
    overflow: hidden;
    padding: 5px;
    width: 200px;
}

.signup-box .signup-content .ct form .field input.text,.signup-box .signup-content .ct form .field textarea.text {
	background-color: #25282B;
	border: medium none;
	height: 30px;
    line-height: 22px;
    color: #F1F1F1;
    font-family: "Segoe UI","Lucida Grande","Lucida Sans Unicode","Lucida Sans","DejaVu Sans","Bitstream Vera Sans","Liberation Sans",Verdana,"Verdana Ref",sans serif;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5) inset;
}

.quiet {
    color: #999999;
}

 .signup-box .signup-content .ct form .quiet {
