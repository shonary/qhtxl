@CHARSET "UTF-8";
body:before {
    content: "";
    float: left;
    height: 100%;
    margin-top: -32767px;
    width: 0;
}

body {
	background: none repeat scroll 0 0 #FFFFFF;
    color: #222222;
    font-family: "Segoe UI","Lucida Grande","Lucida Sans Unicode","Lucida Sans","DejaVu Sans","Bitstream Vera Sans","Liberation Sans",Verdana,"Verdana Ref",sans serif;
    font-size: 75%;
    margin: 0;
	height: 100%;
	line-height: 1.5;
}

body.dark {
    background: url("../img/body_bg_dark.jpg") repeat scroll 0 0 #343942;
}

.page-wrap {
    min-height: 100%;
    width: 100%;
}

.page-container {
    margin: 0;
}

.page-container {
    overflow: visible;
    padding-bottom: 30px;
}

.container_24 {
    margin-left: auto;
    margin-right: auto;
    width: 960px;
}

.grid_24 {
    display: inline;
    float: left;
    margin-left: 5px;
    margin-right: 5px;
}

.login-box{
	left: 50%;
    margin-left: -400px;
    margin-top: -185px;
    position: absolute;
    top: 45%;
    width: 800px;
}

.login-box .logo {
    height: 120px;
    margin-bottom: 50px;
    margin-left: 150px;
    width: 600px;
    background: url("../img/Banner.png") repeat scroll 0px 0px transparent;    
}

.login-box .login-content {
    margin-left: 105px;
    overflow: hidden;
}

.p1,.p2,.p3{
	overflow: hidden;
}

.login-box .login-content .ct form .field {
    float: left;
    margin-bottom: 10px;
    margin-right: 15px;
    overflow: hidden;
    position: relative;
}

.login-box .login-content .ct form .field label{
 	color: #53575B;
    cursor: text;
    font-weight: bold;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 0;
    font-size: 16px;
    height: 44px;
    left: 16px;
    line-height: 44px;
}

.login-box .login-content .ct form .field input.text,.login-box .login-content .ct form .field textarea.text {
    background-color: #25282B;
    border: medium none;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5) inset;
    color: #F1F1F1;
    font-family: "Segoe UI","Lucida Grande","Lucida Sans Unicode","Lucida Sans","DejaVu Sans","Bitstream Vera Sans","Liberation Sans",Verdana,"Verdana Ref",sans serif;
    height: 22px;
    line-height: 22px;
}

.login-box .login-content .ct form .field input.text {
    border: 1px solid rgba(255, 255, 255, 0.4);
    font-size: 16px;
    font-weight: bold;
    height: 45px;
    line-height: 32px;
    padding: 5px 14px;
    width: 270px;
}



.login-box .login-content .ct form .field a.login-submit {
    font-size: 16px;
    height: 42px;
    line-height: 42px;
    overflow: hidden;
}

.middlebutton.loginblue {
    background-color: #3C7FED;
    border: 1px solid #5396FD;
    border-radius: 5px 5px 5px 5px;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.7);
    color: #FFFFFF !important;
    display: inline-block;
    font-size: 14px;
    font-weight: bold;
    height: 30px;
    line-height: 30px;
    margin-bottom: 2px;
    margin-right: 2px;
    padding: 0 14px;
    text-align: center;
    text-shadow: 0 0 2px black;
}

.login-box .login-content .ct form .p2 {
    margin-bottom: 30px;
}

.login-box .login-content .ct form .p1, .login-box .login-content .ct form .p2, s.login-box .login-content .ct form .p3 {
    overflow: hidden;
}

.login-box .login-content .ct form .field.reg {
    font-size: 14px;
    font-weight: bold;
    overflow: hidden;
    width: 270px;
}

.login-box .login-content .ct form .field.reg a {
    color: #F1F1F1;
    height: 20px;
    line-height: 20px;
    padding: 0 4px;
    text-shadow: 0 0 3px black;
}

.login-box .login-content .ct form .field.misc {
    font-size: 14px;
    height: 20px;
    line-height: 20px;
    overflow: hidden;
    width: 270px;
}

.login-box .login-content .ct form .field.misc .remember-me {
    color: #F1F1F1;
    float: left;
    margin-right: 30px;
    padding: 0 4px;
    text-shadow: 0 0 3px black;
}

input.checkbox {
    position: relative;
    top: 2px;
}

* {
    margin: 0;
    padding: 0;
}

.login-box .login-content .ct form .field.misc .forget-password a {
    color: #F1F1F1;
}

a {
    text-decoration: none;
}

.login-box .login-content .ct form .field.tsina {
    overflow: hidden;
    width: 270px;
}

.login-box .login-content .ct .connects .connect .icon {
    float: left;
    height: 24px;
    margin-left: 3px;
    position: relative;
    top: 3px;
    width: 24px;
}

.login-box .login-content .ct .connects .connect.tsina-weibo .icon {
    background: url("../img/share_24.png") no-repeat scroll -4px -4px transparent;
}

.login-box .login-content .ct .connects .connect .name {
    float: left;
    font-size: 14px;
    height: 30px;
    line-height: 30px;
    padding: 0 9px;
}

.login-box .login-content .ct .connects .connect {
    float: left;
    padding: 0 3px;
}

.middlebutton.loginred {
    background-color: #ED513C;
    border: 1px solid #FD6553;
    border-radius: 5px 5px 5px 5px;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.7);
    color: #FFFFFF !important;
    display: inline-block;
    font-size: 14px;
    font-weight: bold;
    height: 30px;
    line-height: 30px;
    margin-bottom: 2px;
    margin-right: 2px;
    padding: 0 14px;
    text-align: center;
    text-shadow: 0 0 2px #444444;
}


/*
 * 脚部
 */

.page-footer {
    clear: both;
    height: 30px;
    margin-top: -30px;
    position: relative;
}

body .page-footer {
    height: 30px;
    line-height: 30px;
    text-align: center;
}

body.dark .page-footer {
    color: #F1F1F1;
    text-shadow: 0 0 3px black;
}

body.dark .page-footer a {
    color: #F1F1F1 !important;
}

/*
 * 注册开始
 */

.signup-box {
    left: 50%;
    margin-left: -280px;
    margin-top: 120px;
    position: absolute;
    width: 600px;
}

.signup-box .logo {
    background: url("../img/Banner.png") no-repeat scroll 0 0 transparent;
    height: 120px;
    margin-bottom: 40px;
    width: 600px;
}

.signup-box .signup-content {
    overflow: hidden;
    padding: 0 14px;
}

.signup-box .signup-content .ct {
    border-right: 1px solid #A5A5A5;
    float: left;
    padding-right: 60px;
}


.signup-box .signup-content .ct form .fi {
    height: 24px;
    margin-bottom: 14px;
    padding: 0 3px;
}

