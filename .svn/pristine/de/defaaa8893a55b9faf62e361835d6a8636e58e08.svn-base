}

 .signup-box .signup-content .others {
    float: right;
    margin-right: 30px;
    margin-top: 15px;
}

.signup-box .signup-content .others .bl {
    margin-bottom: 14px;
}

.signup-box .signup-content .others .desc {
    color: #F1F1F1;
    margin-bottom: 5px;
    text-shadow: 0 0 3px black;
}

.middlebutton.logingray {
    background-color: #666666;
    border: 1px solid #777777;
    border-radius: 5px 5px 5px 5px;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.7);
    color: #FFFFFF !important;
    display: inline-block;
    font-size: 14px;
    font-weight: bold;
    height: 30px;
    line-height: 30px;
    margin-bottom: 2px;
    margin-right: 2px;
    padding: 0 14px;
    text-align: center;
    text-shadow: 0 0 2px black;
}

.signup-box .signup-content .others .connects .connect .icon {
    float: left;
    height: 24px;
    margin-left: 3px;
    position: relative;
    top: 3px;
    width: 24px;
}

.signup-box .signup-content .others .connects .connect .name {
    float: left;
    font-size: 14px;
    height: 30px;
    line-height: 30px;
    padding: 0 9px;
}

.signup-box .signup-content .others .connects {
    overflow: hidden;
}

.signup-box .signup-content .others .connects .connect.tsina-weibo .icon {
    background: url("../img/share_24.png") no-repeat scroll -4px -4px transparent;
}

.signup-box .signup-content .others .connects .connect {
    float: left;
    padding: 0 3px;
}

/*
 *消息提示页面
 */
.message-box {
    left: 50%;
    margin-left: -280px;
    margin-top: 120px;
    position: absolute;
    width: 600px;
}

.message-box .logo {
    background: url("../img/Banner.png") no-repeat scroll 0 0 transparent;
    height: 120px;
    margin-bottom: 40px;
    width: 600px;
}

.message-box .content{
	padding-left: 14px;
}

.message-box .title{
	color: #F1F1F1;
	font-size: 14px;
	font-weight: bold;
	margin-bottom: 10px;
}

.message-box .desc{
	color: #F1F1F1;
	font-size: 14px;	
}

.message-box .desc .loud{
	color: #FBEB96;	
}


/*
 * 注册页面错误提示
 */

.flash-error span {
	color:red;
    display: block;
    font-size: 14px;
    font-weight: bold;
    height: 24px;
    line-height: 24px;
    overflow: hidden;
}
.signup-box .signup-content .ct form .fi {
    height: 24px;
    margin-bottom: 14px;
    padding: 0 3px;
}
